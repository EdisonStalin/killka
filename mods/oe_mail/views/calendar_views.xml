<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE xml>
<odoo>
	<data>
		
		<record id="view_calendar_event_calendar_inherit_oe_mail" model="ir.ui.view">
	        <field name="name">calendar.event.calendar.inherit.oe_mail</field>
	        <field name="model">calendar.event</field>
	        <field name="inherit_id" ref="calendar.view_calendar_event_calendar"/>
	        <field name="arch" type="xml">
	        	
	        	<xpath expr='//calendar' position='attributes'>
	        		<attribute name="mode">month</attribute>
	        		<attribute name="event_open_popup">False</attribute>
	        	</xpath>
	        	
	        </field>
        </record>
        
		
	
	</data>
</odoo>